<style>
	img {
		display: block;
		margin: 0 auto;
	}

	h3 {
		left: 5mm;
	}

	<% // Phantom JS is awful and does not support flexbox properly. Using old style flexbox.. %>
	.part {
		display: flex;
		justify-content: space-between;
		display: -webkit-box;
		-webkit-box-orient: horizontal;
		-webkit-box-pack: justify;
		left: 5mm;
		right: 5mm;
		padding-bottom: 1mm;
		padding-top: 1mm;
		border-bottom: dashed;
		border-bottom-width: 1px;
		border-bottom-color: rgba(0, 0, 0, 0.25);
	}

	.partDescription {
		display: flex;
		justify-content: space-between;
		display: -webkit-box;
		-webkit-box-orient: horizontal;
		-webkit-box-pack: justify;
		top: 3mm;
		bottom: 3mm;
		left: 5mm;
		right: 5mm;
		border-bottom: solid;
		border-bottom-width: 3px;
	}

	.warning {
		color : red;
	}

</style>

<img class='logo' src="<%= logo %>" width="75%" align="middle">

<h1 align="center" class="warning">THIS IS AN EXAMPLE TO FIX LINK IN PROD</h1>

<h1 align="center"><%= title %></h1>

<h3>Client Name: <%= client %> </h3>
<h3>Email: <%= email %> </h3>

<div class="partDescription">
	<div class="left">Part Number</div>
	<div class="right">Quantity</div>
</div>
<% for (var prop in parts) {
     if (Object.prototype.hasOwnProperty.call(parts, prop)) { %>
		<div class="part">
			<div class="left"><a href="http://wirewerks.mr-lvv.com/state/product/<%= prop %>"><%= prop %></a> </div>
			<div class="right"><%= parts[prop].quantity %></div>
		</div>
<%   }
} %>


